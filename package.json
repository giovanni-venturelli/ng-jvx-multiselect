    "@types/jasmine": "~3.6.0",
    "@types/node": "^20.10.6",
    "codelyzer": "^6.0.2",
    "jasmine-core": "3.8.0",
    "karma": "~6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^18.0.0",
    "protractor": "~7.0.0",
    "scss-bundle": "^3.1.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^5.4.5"
